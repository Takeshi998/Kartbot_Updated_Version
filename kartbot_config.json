{
    "prefix": "k$",
    "description": "Bot de SRB2Kart.",
    "token": "copie su token de su bot aca",

    "screen_name": "server1",
    "server_folder_path": "/home/ubuntu/.srb2kart/",
    "server_executable_name": "srb2kart",
    "server_executable_args": "-home /home/ubuntu/ -port 5029 -dedicated +advertise 1 -file",
    "server_max_players": 16,
    "ip_message": "```Race 127.0.0.1```",
    "permission_error_message": "Solo los miembros con los roles ADM, Webmaster o Helper pueden usar este comando",
    "admin_roles": ["ADM", "Webmaster"],

    "enable_dkartconfig_corruption_workaround": true,
    "enable_log_backup": true,
    "backup_dkartconfig_path": "/home/ubuntu/.srb2kart/dkartconfig.cfg.bak",
    "dkartconfig_path": "/home/ubuntu/.srb2kart/dkartconfig.cfg",
    "log_path": "/home/ubuntu/.srb2kart/log.txt",
    "backup_log_path": "/home/ubuntu/.srb2kart/log.txt.bak",
    "tag_path": "/home/ubuntu/karbot_SKDX/tags/",

    "chat_bridge": true,
    "chat_bridge_channel_id": [id del canal de su bridge],
    "bot_commands_channel_id": [canal donde se podran utilizar comandos del bot],
    "webhook_url": "especifique su webhook de su chat_bridge_channel_id",
    "webhook_base_avatar_url": "https://kart.raphaelgoul.art/static/webhook_avatars/"
}
